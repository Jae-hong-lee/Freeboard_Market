{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/boarder/[boardId]/edit/index.js",
    "pages/boarder/[boardId]/index.js",
    "src/components/units/boards/boardComment/write/BoardCommentWrite.quires.js",
    "src/components/units/boards/boardComment/write/BoardCommentWrite.presenter.js",
    "src/components/units/boards/boardComment/write/BoardCommentWrite.container.js"
  ],
  "exclude": ["node_modules"]
}
